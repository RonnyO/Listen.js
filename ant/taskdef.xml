<project name="taskdef" basedir=".">

	<!-- Set ant classpath -->
	<path id="jars.claspath">
		<fileset dir="${basedir}/lib">
			<include name="**/*.jar" />
		</fileset>
	</path>

	<!-- Add needed tasks -->
	<taskdef resource="net/sf/antcontrib/antcontrib.properties" classpathref="jars.claspath" />

</project>
